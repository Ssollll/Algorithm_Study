    "n = int(input())\n",
    "array = []\n",
    "for i in range(n):\n",
    "    n, m = map(str, input().split())\n",
    "    array.append((n, int(m)))\n",
    "    \n",
    "array.sort(key = lambda x: x[1])\n",
    "for i in array:\n",
    "    print(i[0], end = ' ')"
